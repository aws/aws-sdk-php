        "response": {
          "status": 200,
          "headers": {
            "ETag": "\"winhappy456\""
          }
        }
      },
      {
        "request": {
          "operation": "PutObject",
          "bucket": "example-bucket",
          "key": "backup/notes.txt",
          "filePath": "C:\\Users\\user\\Documents\\notes.txt"
        },
        "response": {
          "status": 200,
          "headers": {
            "ETag": "\"winhappy789\""
          }
        }
      }
    ],
    "outcomes": [
      {
        "result": "success",
        "objectsUploaded": 3,
        "objectsFailed": 0,
        "note": "Windows normal operation with backslash to forward slash conversion"
      }
    ]
  },
