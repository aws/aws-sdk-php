        "type": "file",
        "size": 2048
      }
    ],
    "expectations": [
      {
        "request": {
          "operation": "PutObject",
          "bucket": "example-bucket",
          "key": "backup/report.docx",
          "filePath": "C:\\Users\\user\\Documents\\report.docx"
        },
        "response": {
          "status": 200,
          "headers": {
            "ETag": "\"winhappy123\""
          }
        }
      },
      {
        "request": {
          "operation": "PutObject",
          "bucket": "example-bucket",
          "key": "backup/projects/presentation.pptx",
